{
  "sfmc-domain": "{{sfmc-subdomain}}",
  "sfmc-client-id": "{{package-client-id}}",
  "sfmc-mid": "{{only-if-needed}}",
  "public-domain": "{{publically-accessible-domain}}",

  "port": 4000,
  "dev-folder-path": "",
  "files-to-template": [ ],
  "automate-version": true,
  "dev-tokens": {
    "token-name": "use {{Mustache}} syntax in your files to fill your dev files"
  },
  "prod-tokens": {
    "token-name": "use {{Mustache}} syntax in your files to fill your prod files"
  },
  "proxy-any-file": {
    "main-path": "/all-in-dev",
    "enabled": true,
    "dev-token": "{{dev-token-comes-here}}" 
  },
  "proxy-endpoints": [
    {
      "path": "path/to/proxy",
      "targetOverride": "https://override-server.here",
      "pathRewrite": "/proxy/path/with"
    }
  ]
}